   "source": [
    "using Plots\n",
    "\n",
    "x = collect(range(0, stop=5, length = 20))\n",
    "y = x.^2\n",
    "\n",
    "th = atan.(2 .*x) .- pi/2\n",
    "\n",
    "plot(x, y)\n",
    "quiver!(x, y, quiver=(sin.(th), cos.(th)))"
   ]
